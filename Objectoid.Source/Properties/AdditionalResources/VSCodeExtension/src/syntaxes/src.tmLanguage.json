{
  "name": "SRC",
  "fileTypes": [
    ".src"
  ],
  "scopeName": "text.src",
  "patterns": [
    { "include": "#main" }
  ],
  "repository": {
    "main": {
      "patterns": [
        { "include": "#comment" },
        { "include": "#keyword" },
        { "include": "#numeric" },
        { "include": "#string" },
        { "include": "#constant" },
        { "include": "#operator" }
      ]
    },
    "comment": {
      "begin": "\\#",
      "end": "[\\r\\n]",
      "name": "comment.line.number-sign"
    },
    "keyword": {
      "begin": "\\@",
      "end": "\\s|\\x5C|\\x3A",
      "name": "keyword.control"
    },
    "numeric": {
      "begin": "[\\x2D[\\x30-\\x39]]",
      "end": "\\s|\\x5C|\\x3A",
      "name": "constant.numeric"
    },
    "string": {
      "begin": "\\x22",
      "end": "[\\x22\\r\\n]",
      "name": "string.quoted.double",
      "patterns": [
        {
          "name": "constant.character.escape",
          "match": "\\x5C(\\x75[[\\x30-\\x39][\\x41-\\x46][\\x61-\\x66]]{4}|.)"
        }
      ]
    },
    "constant": {
      "begin": "[[\\x41-\\x5A][\\x61-\\x7A]]",
      "end": "\\s|\\x5C|\\x3A",
      "name": "constant.language"
    },
    "operator": {
      "match": "\\x5C|\\x3A",
      "name": "keyword.operator"
    }
  }
}